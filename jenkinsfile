pipeline {
    agent none

    stages {
        stage('Back-end') {
            agent {
                docker { image 'maven:3.8.1-adoptopenjdk-11' }
            }
            steps {
                sh 'mvn --version'
                sh 'javac Main.java'   // Compile Java file
                sh 'java Main'         // Run Java file
            }
        }
    }
}
